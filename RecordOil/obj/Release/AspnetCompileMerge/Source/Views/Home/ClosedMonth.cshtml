@model RecordOil.Models.Zayavka

<div class="modal-header" style="background-color: #FFE4B5; border-radius: 5px;">
    <button class="close" data-dismiss="modal" area-hidden="true"><i class="fa fa-close"></i></button>
    <h5 style="text-align:center">Закрытие месяца</h5>
</div>
<div class="modal-body" style="text-align: center">


    <h5>Вы точно хотите закрыть завки?</h5>

</div>
<div class="modal-footer" style="text-align: center">
    <button type="submit" class="btn btn-warning" style="min-width:100px" onclick="ClosedZayavkaOK()">Да</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Нет</button>
</div>







